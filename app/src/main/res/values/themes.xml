<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SwiftPark" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryVariant">@color/black</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#a82d65</item>
        <item name="colorSecondaryVariant">#a82d65</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>

        <!-- Customize your theme here. -->

        <item name="android:fontFamily">@font/dm_sans_medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16dp</item>
        <item name="android:background">#eeeeee</item>
    </style>
</resources>